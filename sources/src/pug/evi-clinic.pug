                        var imgSrcset = "./images/scr_opisanie_keysa_art6/evi_clinic_preview1_400.webp 400w, ./images/scr_opisanie_keysa_art6/evi_clinic_preview1_700.webp 700w, ./images/scr_opisanie_keysa_art6/evi_clinic_preview1_900.webp 900w, ./images/scr_opisanie_keysa_art6/evi_clinic_preview1_1280.webp 1280w, ./images/scr_opisanie_keysa_art6/evi_clinic_preview1_1500.webp 1500w, ./images/scr_opisanie_keysa_art6/evi_clinic_preview1_2560.webp 2560w, ./images/scr_opisanie_keysa_art6/evi_clinic_preview1_3000.webp 3000w"

